<div class="container">
  <% @obstetricians.each do |obstetrician| %>
    <div class="row">
      <%= image_fu obstetrician.photo, nil, class: "pull-#{cycle("left", "right")}" %>
      <h3>
        <%= "#{obstetrician.name}, #{obstetrician.title}" %>
      </h3>
      <p><%=raw obstetrician.description %></p>
      <blockquote><%=raw obstetrician.quote %></blockquote>
    </div>
  <% end %>
</div>
