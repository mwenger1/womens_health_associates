  <div class="row">
    <div class="col-md-12">
      <%= render '/refinery/content_page' %>
    </div>
  </div>

  <% @obstetricians.each do |obstetrician| %>
    <hr class="featurette-divider">

    <div class="row featurette">
      <div class="col-md-9 pull-<%= cycle("left", "right") %>">
        <h2 class="featurette-heading"><%= obstetrician.name %>, <span class="text-muted"><%= obstetrician.title %></span></h2>
        <p class="lead"><%=raw obstetrician.description %></p>
      </div>
      <div class="col-md-3">
        <%= image_fu obstetrician.photo, nil, class: "featurette-image img-reponsive center-block" %>
      </div>
    </div>
  <% end %>
