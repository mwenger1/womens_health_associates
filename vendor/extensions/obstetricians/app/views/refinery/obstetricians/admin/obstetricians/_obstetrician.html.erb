<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(obstetrician) -%>">
  <span class='title'>
    <%= obstetrician.name %>

  </span>
  <span class='actions'>
    
      <%= action_icon(:preview, refinery.obstetricians_obstetrician_path(obstetrician), t('.view_live_html')) %>
    
    <%= action_icon(:edit,    refinery.edit_obstetricians_admin_obstetrician_path(obstetrician), t('.edit') ) %>
    <%= action_icon(:delete,  refinery.obstetricians_admin_obstetrician_path(obstetrician), t('.delete'),
      { class: "cancel confirm-delete",
        data: {confirm: t('message',  scope: 'refinery.admin.delete', title: obstetrician.name)}}  ) %>
  </span>
</li>
