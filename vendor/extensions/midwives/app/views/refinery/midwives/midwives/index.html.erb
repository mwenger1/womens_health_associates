<div class="container">
  <div class="row">
    <%= render '/refinery/content_page' %>
  </div>

  <% @midwives.each do |midwife| %>
    <div class="row">
      <%= image_fu midwife.photo, nil, class: "pull-#{cycle("left", "right")}" %>
      <h3>
        <%= "#{midwife.name}, #{midwife.title}" %>
      </h3>
      <p><%=raw midwife.description %></p>
    </div>
  <% end %>
</div>
